        "import heapq\n",
        "def astar(start):\n",
        "  pq = [(manhattan(start),0, start)]\n",
        "  visited = set()\n",
        "  while pq:\n",
        "    f,g, state = heapq.heappop(pq)\n",
        "    if state == goal:\n",
        "      print(\"Solved!\")\n",
        "      return\n",
        "    if state in visited:\n",
        "      continue\n",
        "    visited.add(state)\n",
        "    for n in get_neighbors(state):\n",
        "      heapq.heappush(pq, (g+1+manhattan(n), g+1, n))\n",
        "start=(1,2,3,4,0,5,6,7,8)\n",
        "astar(start_state)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "goal = (1, 2, 3, 4, 5, 6, 7, 8, 0)\n",
        "def manhattan(state):\n",
        "    dist = 0\n",
        "    for i in range(9):\n",
        "        if state[i] != 0:\n",
        "            goal_pos = goal.index(state[i])\n",
        "            dist += abs(i // 3 - goal_pos // 3) + abs(i % 3 - goal_pos % 3)\n",
        "    return dist\n",
        "\n",
        "def get_neighbors(state):\n",
        "    neighbors = []\n",
        "    idx = state.index(0)\n",
        "    moves = [-3, 3, -1, 1]\n",
        "    for m in moves:\n",
        "        new_idx = idx + m\n",
        "        if 0 <= new_idx < 9:\n",
        "            if idx % 3 == 2 and m == 1:\n",
        "                continue\n",
        "            if idx % 3 == 0 and m == -1:\n",
        "                continue\n",
        "            new_state = list(state)\n",
        "            new_state[idx], new_state[new_idx] = new_state[new_idx], new_state[idx]\n",
        "            neighbors.append(tuple(new_state))\n",
        "    return neighbors\n",
        "\n",
        "import heapq\n",
        "def astar(start):\n",
        "  pq = [(manhattan(start),0, start)]\n",
        "  visited = set()\n",
        "  while pq:\n",
        "    f,g, state = heapq.heappop(pq)\n",
        "    if state == goal:\n",
        "      print(\"Solved!\")\n",
        "      return\n",
        "    if state in visited:\n",
        "      continue\n",
        "    visited.add(state)\n",
        "    for n in get_neighbors(state):\n",
        "      heapq.heappush(pq, (g+1+manhattan(n), g+1, n))\n",
        "start=(1,2,3,4,0,5,6,7,8)\n",
        "astar(start_state)\n",
        "\n"
      ],
