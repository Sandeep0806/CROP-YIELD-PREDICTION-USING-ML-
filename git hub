      "source": [
        "from collections import  deque\n",
        "def water_jug():\n",
        "  start=(0,0)\n",
        "  goal=2\n",
        "  visited=set()\n",
        "  q = deque([(start,[start])])\n",
        "  while q:\n",
        "    (x,y),path=q.popleft()\n",
        "    if x == goal or y==goal:\n",
        "       print(\"Goal reached:\", (x,y))\n",
        "       print (\"Action sequence (states):\")\n",
        "    for step in path:\n",
        "      print(step)\n",
        "    return"
      ],
